
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and are for development purposes only.
    // They allow any authenticated user to read, write, and delete any data.
    // Secure rules must be implemented before deploying to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
